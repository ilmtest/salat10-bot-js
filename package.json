{
    "name": "salat10-bot",
    "version": "1.1.1",
    "description": "Telegram bot to calculate prayer times",
    "scripts": {
        "clean": "git branch | grep -v '^*' | xargs git branch -D",
        "start": "ENV=dev nodemon index",
        "test": "NTBA_FIX_319=1 CONTACT_CHAT_ID=123 jest",
        "prepush": "npm test",
        "zip": "rm -rf nodejs.zip salat10-bot-js.zip && zip -9 -r salat10-bot-js.zip index.js package.json src && mkdir -p nodejs && cp -R node_modules nodejs/node_modules && zip -9 -r nodejs.zip nodejs && rm -rf nodejs"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ilmtest/salat10-bot-js.git"
    },
    "keywords": [
        "ilmtest",
        "islam",
        "muslim",
        "salafi",
        "salaf",
        "javascript",
        "es6",
        "node",
        "nodejs",
        "express",
        "developer",
        "development"
    ],
    "author": "IlmTest",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ilmtest/salat10-bot-js/issues"
    },
    "homepage": "https://github.com/ilmtest/salat10-bot-js#readme",
    "dependencies": {
        "@ilmtest/salat10-sdk": "1.0.1",
        "@sentry/node": "5.22.3",
        "analytics-node": "3.4.0-beta.2",
        "dotenv": "8.2.0",
        "node-telegram-bot-api": "0.50.0"
    },
    "devDependencies": {
        "eslint": "7.8.1",
        "eslint-config-airbnb-base": "14.2.0",
        "eslint-config-prettier": "6.11.0",
        "eslint-plugin-import": "2.22.0",
        "eslint-plugin-prettier": "3.1.4",
        "hat": "0.0.3",
        "jest": "25.5.4",
        "nodemon": "2.0.4",
        "prettier": "2.1.1"
    }
}
